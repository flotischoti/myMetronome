{
  "name": "metronomes",
  "version": "1.0.0",
  "description": "Manage different metronomes to track your practice progress",
  "main": "index.js",
  "scripts": {
    "postinstall": "prisma generate",
    "dev": "next dev",
    "dev:db:start": "docker-compose -f docker-compose.dev.yml up -d && npx prisma migrate deploy",
    "dev:db:stop": "docker-compose -f docker-compose.dev.yml down",
    "build": "next build",
    "start": "node .next/standalone/server.js",
    "docker:up": "docker-compose up",
    "docker:up:build": "docker-compose up --build",
    "docker:down": "docker-compose down",
    "db:migrate": "npx prisma migrate dev",
    "db:seed": "npx prisma db seed",
    "db:reset": "npx prisma migrate reset --force",
    "lint": "next lint",
    "test": "jest"
  },
  "author": "FlotiSchoti",
  "license": "GPL-3.0 license",
  "dependencies": {
    "@prisma/client": "^5",
    "@tabler/icons-react": "^2",
    "@types/bcrypt": "^5",
    "bcrypt": "^5",
    "jose": "^5",
    "next": "^14",
    "prisma": "^5",
    "react": "^18",
    "react-dom": "^18",
    "resend": "^6.5.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^8",
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10",
    "daisyui": "^4",
    "eslint": "8",
    "eslint-config-next": "14",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "node-fetch": "^3",
    "npm-run-all": "^4",
    "postcss": "^8",
    "prettier": "3",
    "sass": "^1",
    "tailwindcss": "^3",
    "ts-jest": "^29.4.5",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "prisma": {
    "seed": "ts-node --esm prisma/seed.mts"
  }
}
