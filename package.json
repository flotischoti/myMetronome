{
  "name": "mymetronome",
  "version": "1.0.0",
  "description": "Manage different metronomes to track your practice progress",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "npx prisma migrate dev",
    "seed": "npx prisma db seed",
    "docker:stop": "docker stop metroDb",
    "docker:remove": "docker rm metroDb",
    "docker:run": "docker run --rm --name metroDb -p 5432:5432 -e POSTGRES_USER=user -e POSTGRES_PASSWORD=password -e POSTGRES_DB=metronome -d postgres",
    "docker:start": "docker start metroDb",
    "db:startup": "npm-run-all -s -c docker:run docker:start migrate",
    "db:refresh": "npm-run-all -s -c db:startup db:init",
    "db:init": "npx prisma migrate reset --force",
    "dev:clean": "npm-run-all -s -c db:refresh next",
    "dev": "npm-run-all -s -c db:startup next",
    "next": "next dev",
    "build": "next build",
    "start": "next start",
    "prepare": "husky install"
  },
  "author": "FlotiSchoti",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@prisma/client": "^4.8.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.0",
    "eslint-config-next": "^13.1.3",
    "jsonwebtoken": "^9.0.0",
    "next": "^13.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@tailwindcss/forms": "^0.5.3",
    "@types/node": "18.11.17",
    "@types/react": "18.0.26",
    "autoprefixer": "^10.4.13",
    "dotenv": "^16.0.3",
    "eslint": "8.30.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "node-fetch": "^3.3.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "prettier": "2.8.1",
    "prisma": "^4.8.0",
    "sass": "^1.57.0",
    "tailwind-merge": "^1.8.1",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "4.9.4"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write"
  },
  "prisma": {
    "seed": "ts-node --esm prisma/seed.mts"
  }
}
